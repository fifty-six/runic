extern print (x: i32) unit

fn main -> unit = do {
    call print (call fac 5);
    call print (call fib 5);
}

fn fib (x: i32) -> i32 = 
    if (<= x 1) 
        do { 1; }
        (+ 
            (call fib (- x 1))
            (call fib (- x 2))
        )


fn fac (x: i32) -> i32 = 
    if (and (<= x 1) do { call print 11; true; })
        1 
        (* x (call fac (- x 1)))

